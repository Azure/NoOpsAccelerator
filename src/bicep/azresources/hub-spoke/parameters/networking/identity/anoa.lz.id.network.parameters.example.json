{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "parOrgPrefix": {
            "value": "anoa"
        },
        "parTemplateVersion": {
            "value": "v1.0"
        },
        "parIdentitySubscriptionId": {
            "value": "xxxxxxxx-xxxxxx-xxxxx-xxxxxx-xxxxxx"
        },
        "parDeployEnvironment": {
            "value": "workload"
        },       
        "parIdentityVirtualNetworkAddressPrefix": {
            "value": "0.0.0.0/26"
        },
        "parIdentitySubnetAddressPrefix": {
            "value": "0.0.0.0/27"
        }, 
        "parFirewallPrivateIPAddress": {
            "value": "0.0.0.0"
        },        
        "enableActivityLogging": {
            "value": true
        },        
        "parAddRoleAssignmentForStorageAccount": {
            "value": true
        },
        "parStorageAccountAccessObjectId": {
            "value": "xxxxxxxx-xxxxxx-xxxxx-xxxxxx-xxxxxx"
        },
        "parStorageAccountAccessType": {
            "value": "Group"
        },
        "parLogAnalyticsWorkspaceResourceId": {
            "value": "/subscriptions/xxxxxxxx-xxxxxx-xxxxx-xxxxxx-xxxxxx/resourcegroups/anoa-eastus-workload-logging-rg/providers/microsoft.operationalinsights/workspaces/anoa-eastus-workload-logging-log"
        },
        "parLogAnalyticsWorkspaceName": {
            "value": "anoa-eastus-workload-logging-log"
        }
    }
}